@{
    ViewData["Title"] = "History";
}

<h1>History</h1>
<table class="table sortable">
    <tr>
        <th>Domain</th>
        <th>Pages</th>
        <th>Elapsed</th>
        <th>Completed</th>
    </tr>
    @foreach (var result in ViewBag.Message.Results)
    {
        <tr>
            <td><a href="https://@ViewBag.Message.Domain/results?guid=@result.Guid">@result.Information.Domain</a>
            <td>@result.Information.Pages</td>
            <td>@result.Information.Elapsed</td>
            <td>@result.Information.Completion</td>
        </tr>
    }
</table>

@section Scripts{
    <script src="~/js/sorttable.js"></script>
}